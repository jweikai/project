}
/* 用户列表  */
.user-list .fresh-btn{
	margin-top: 4px;
	color: #999;	
	transition: 0.25s transform;
	-webkit-transition: 0.25s transform;
	-moz-transition: 0.25s transform;
	-o-transition: 0.25s transform;
	cursor: pointer;	
}
.user-list .fresh-btn:hover {
	transform: rotate(45deg);
	-webkit-transform: rotate(45deg);
	-moz-transform: rotate(45deg);
	-o-transform: rotate(45deg);
}
.user-list .fresh-btn.run {
	animation: ascircle 0.75s infinite linear;
	-webkit-animation: ascircle 0.75s infinite linear;
	-moz-animation: ascircle 0.75s infinite linear;
	-o-animation: ascircle 0.75s infinite linear;
}
.user-list img {
	width: 40px;
}


/* video comment */
.video-comment .nav-tabs > li.active {	
	border-bottom: 2px solid #00AAEE;
}
.video-comment .nav-tabs > li.active a {
	border: 0;
	color: #00AAEE;
}
.video-comment .nav-tabs > li a:hover {
	background: transparent;	
	color: #00AAEE;
}
.video-comment .nav-tabs > li > a {
	color: #222;
	padding: 0 40px 10px 40px;
	border: 0;
	background: #fff;	
}
.video-comment .panel-body{
	padding-left: 15px;
}
.comment-btn {
	padding: 20px 0;
}
.comment-btn button {
	padding-left: 30px;
	padding-right: 30px;
}
.video-comment .media-left img {	
	width: 40px;
	margin-right: 10px;
}
.video-comment .media-body {
	vertical-align: middle;
	width: 100%;	
}
.video-comment .media-body h4 {
	font-size: 12px;
}
.video-comment .media-bottom {
	color: #aaa;
	font-size: 12px;
	margin-top: 8px;
}
.video-comment .media-bottom .course-from{
	display: inline-block;
	margin-left: 30px;
	cursor: pointer;
}
.video-comment .media-bottom .course-from:hover {
	color: #222;
}
.video-comment .media-bottom .zan {
	float: right;
	cursor: pointer;
	font-size: 16px;	
}
.video-comment .media-bottom .zan:hover {
	color: #5cb85c;	
}
.video-comment .media-bottom .zan.active {
	opacity: 1;
	transform: scale(1);
	-webkit-transform: scale(1);
	-moz-transform: scale(1);
	-o-transform: scale(1);
	color: #5CB85C;
}
.video-comment .media-bottom .zan.run {	
	animation:zanframe 1s 1 ease-in;
	-webkit-animation:zanframe 1s 1 ease-in;
	-moz-animation: zanframe 1s 1 ease-in;
	-o-animation:zanframe 1s 1 ease-in;	
}
@keyframes zanframe{
	from{
		opacity: 0.2;
		transform: scale(1.2);
		-webkit-transform: scale(1.2);
		-moz-transform: scale(1.2);
		-o-transform: scale(1.2);
		color: green;
	}
	to{
		opacity: 1;
		transform: scale(1);
		-webkit-transform: scale(1);
		-moz-transform: scale(1);
		-o-transform: scale(1);
		color: #5CB85C;
	}
}
@-webkit-keyframes zanframe{
	from{
		opacity: 0.2;
		transform: scale(1.2);
		-webkit-transform: scale(1.2);
		-moz-transform: scale(1.2);
		-o-transform: scale(1.2);
		color: green;
	}
	to{
		opacity: 1;
		transform: scale(1);
		-webkit-transform: scale(1);
		-moz-transform: scale(1);
		-o-transform: scale(1);
		color: #5CB85C;
	}
}
@-moz-keyframes zanframe{
	from{
		opacity: 0.2;
		transform: scale(1.2);
		-webkit-transform: scale(1.2);
		-moz-transform: scale(1.2);
		-o-transform: scale(1.2);
		color: green;
	}
	to{
		opacity: 1;
		transform: scale(1);
		-webkit-transform: scale(1);
		-moz-transform: scale(1);
		-o-transform: scale(1);
		color: #5CB85C;
	}
}
@-o-keyframes zanframe{
	from{
		opacity: 0.2;
		transform: scale(1.2);
		-webkit-transform: scale(1.2);
		-moz-transform: scale(1.2);
		-o-transform: scale(1.2);
		color: green;
	}
	to{
		opacity: 1;
		transform: scale(1);
		-webkit-transform: scale(1);
		-moz-transform: scale(1);
		-o-transform: scale(1);
		color: #5CB85C;
	}
}
.video-comment .media {
	padding-bottom: 20px;
